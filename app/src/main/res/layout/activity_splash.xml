<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context="fr.wildcodeschool.variadis.SplashActivity">
    <ImageView
        android:id="@+id/img_background"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:alpha="0.5"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/img_toulouse" />

    <ImageView
        android:id="@+id/img_tree3"
        android:layout_width="125dp"
        android:layout_height="122dp"
        android:layout_marginTop="24dp"
        android:keyboardNavigationCluster="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_logo"
        app:srcCompat="@drawable/tree_one" />

    <ImageView
        android:id="@+id/img_marker3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="19dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_tree"
        app:srcCompat="@drawable/ic_action_marqueur" />

    <ImageView
        android:id="@+id/img_marker5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="48dp"
        android:layout_marginTop="19dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_tree3"
        app:srcCompat="@drawable/ic_action_marqueur" />

    <ImageView
        android:id="@+id/img_marker7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="32dp"
        app:layout_constraintStart_toEndOf="@+id/img_marker6"
        app:layout_constraintTop_toBottomOf="@+id/img_defi"
        app:srcCompat="@drawable/ic_action_marqueur" />

    <ImageView
        android:id="@+id/img_marker8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="68dp"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_marker1"
        app:srcCompat="@drawable/ic_action_marqueur" />

    <ImageView
        android:id="@+id/img_marker9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="59dp"
        app:layout_constraintStart_toEndOf="@+id/img_marker4"
        app:layout_constraintTop_toBottomOf="@+id/img_tree"
        app:srcCompat="@drawable/ic_action_marqueur" />

    <ImageView
        android:id="@+id/img_marker"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="40dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toStartOf="@+id/img_marker8"
        app:layout_constraintTop_toBottomOf="@+id/img_defi"
        app:srcCompat="@drawable/ic_action_marqueur" />

    <ImageView
        android:id="@+id/img_marker6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_marker9"
        app:srcCompat="@drawable/ic_action_marqueur" />

    <ImageView
        android:id="@+id/img_marker1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="40dp"
        android:layout_marginTop="59dp"
        app:layout_constraintEnd_toStartOf="@+id/img_marker2"
        app:layout_constraintTop_toBottomOf="@+id/img_tree3"
        app:srcCompat="@drawable/ic_action_marqueur" />

    <ImageView
        android:id="@+id/img_marker2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="83dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_tree3"
        app:srcCompat="@drawable/ic_action_marqueur" />

    <ImageView
        android:id="@+id/img_logo"
        android:layout_width="266dp"
        android:layout_height="239dp"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_variadis" />

    <ImageView
        android:id="@+id/img_tree"
        android:layout_width="125dp"
        android:layout_height="121dp"
        android:layout_marginTop="24dp"
        android:keyboardNavigationCluster="true"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_logo"
        app:srcCompat="@drawable/tree_two" />

    <ImageView
        android:id="@+id/img_tree2"
        android:layout_width="125dp"
        android:layout_height="122dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:keyboardNavigationCluster="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_logo"
        app:srcCompat="@drawable/tree_three" />

    <ImageView
        android:id="@+id/img_marker4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="83dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_tree"
        app:srcCompat="@drawable/ic_action_marqueur" />

    <ImageView
        android:id="@+id/img_defi"
        android:layout_width="44dp"
        android:layout_height="47dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/img_marker1"
        app:layout_constraintHorizontal_bias="0.661"
        app:layout_constraintStart_toEndOf="@+id/img_marker3"
        app:layout_constraintTop_toBottomOf="@+id/img_tree"
        app:layout_constraintVertical_bias="0.04"
        app:srcCompat="@drawable/ic_action_defi" />
</android.support.constraint.ConstraintLayout>
